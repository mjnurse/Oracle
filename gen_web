help_line="$(grep '^-- help_line' $1 | sed 's/-- help_line: *//; s/ *$//')"

dest=$(echo "/c/MJN/github/mjnurse-github-io/ORacle-R/Script $1 - $help_line.md" | sed "s/ /_/g")

echo "---" > $dest
echo "title: $1 - $help_line" >> $dest
echo "---" >> $dest
echo "" >> $dest
echo "\`\`\`sql" >> $dest
cat $1 >> $dest
echo "\`\`\`" >> $dest


cat $dest
